); " >&nbsp;&nbsp;int&nbsp;&nbsp;&nbsp;i,j=3DpolySides-1&nbsp;;</span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; color:rgb(51,51,51); font-weight:normal; font-style:normal; font-size:13.5000pt; font-family:'微软雅黑'; background:rgb(255,255,255); mso-shading:rgb(255,255,255); " >&nbsp;&nbsp;bool&nbsp;&nbsp;oddNodes=3DNO&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;;</span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; color:rgb(51,51,51); font-weight:normal; font-style:normal; font-size:13.5000pt; font-family:'微软雅黑'; background:rgb(255,255,255); mso-shading:rgb(255,255,255); " >&nbsp;&nbsp;for&nbsp;(i=3D0;i&#60;polySides;&nbsp;i++)&nbsp;{</span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; color:rgb(51,51,51); font-weight:normal; font-style:normal; font-size:13.5000pt; font-family:'微软雅黑'; background:rgb(255,255,255); mso-shading:rgb(255,255,255); " >&nbsp;&nbsp;&nbsp;&nbsp;if(polyY[i]&#60;y&nbsp;&&&nbsp;polyY[j]&#62;=3Dy</span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; color:rgb(51,51,51); font-weight:normal; font-style:normal; font-size:13.5000pt; font-family:'微软雅黑'; background:rgb(255,255,255); mso-shading:rgb(255,255,255); " >&nbsp;&nbsp;&nbsp;&nbsp;||&nbsp;&nbsp;polyY[j]&#60;y&&&nbsp;polyY[i]&#62;=3Dy)&nbsp;{</span><span style=3D"mso-spacerun:'yes'; font-size:12.0000pt; font-family:'宋体'; " ><br></span><span style=3D"mso-spacerun:'yes'; color:rgb(51,51,51); font-weight:normal; font-style:normal; font-size:13.5000pt; font-family:'微软雅黑'; background:rgb(255,255,255); mso-shading:rgb(255,255,255); " >&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if(polyX[i]+(y-polyY[i])/(polyY[j]-polyY[i])*(polyX[j]-pol