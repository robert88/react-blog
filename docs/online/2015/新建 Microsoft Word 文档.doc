　　　	if ( !prop ) { prop = obj; obj = this; }
　　　
　　　	// Extend the base object
　　　	for ( var i in prop ) obj[i] = prop[i];
　　　
　　　	// Return the modified object
　　　	return obj;
　　　};
　　　
　　　
　　　
　　　
